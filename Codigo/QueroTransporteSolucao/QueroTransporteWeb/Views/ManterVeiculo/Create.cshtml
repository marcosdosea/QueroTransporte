@model QueroTransporte.Model.VeiculoModel

@{
    ViewData["Title"] = "Create";
}

<h2>Create</h2>

<h4>VeiculoModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Id" class="control-label"></label>
                <input asp-for="Id" class="form-control" />
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdFrota" class="control-label"></label>
                @*ViewBag.RotaList is holding all the routs values*@
                @Html.DropDownListFor(model => model.IdFrota, ViewBag.Frotas as SelectList, new
                { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.IdFrota, "", new { @class = "text-danger" })

            </div>
            <div class="form-group">
                <label asp-for="Placa" class="control-label"></label>
                <input asp-for="Placa" class="form-control" />
